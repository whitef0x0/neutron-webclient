#: src/app/templates/modals/hotkey.tpl.html:12
msgctxt "Info"
msgid "<kbd>?</kbd> Opens the <strong>help modal</strong>."
msgstr "<kbd>?</kbd> <strong>Yardım penceresini</strong> açar."

#: src/app/templates/modals/hotkey.tpl.html:35
msgctxt "Info"
msgid "<kbd>.</kbd> Marks or unmarks the message/conversation as <strong>starred</strong>."
msgstr "<kbd>.</kbd> İletiyi/ileti dizisini <strong>yıldızlı</strong> olarak işaretler veya kaldırır."

#: src/app/templates/modals/hotkey.tpl.html:30
msgctxt "Info"
msgid "<kbd>*</kbd> then <kbd>a</kbd> <strong>Select all</strong> conversations."
msgstr "<kbd>*</kbd> ve <kbd>a</kbd> Konuşmaların <strong>tümünü</strong> seçer."

#: src/app/templates/modals/hotkey.tpl.html:31
msgctxt "Info"
msgid "<kbd>*</kbd> then <kbd>n</kbd> <strong>Unselect all</strong> conversations."
msgstr "<kbd>*</kbd> ve <kbd>n</kbd> Tüm konuşmaların <strong>seçimini kaldır</strong>."

#: src/app/templates/modals/hotkey.tpl.html:13
msgctxt "Info"
msgid "<kbd>/</kbd> Focus the <strong>search input</strong>."
msgstr "<kbd>/</kbd> <strong>Arama sorgusuna</strong> odaklanır."

#: src/app/templates/modals/hotkey.tpl.html:62
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marks the <strong>next</strong> message/conversation."
msgstr "<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> <strong>Sonraki</strong> mesajı/ileti dizisini işaretler."

#: src/app/templates/modals/hotkey.tpl.html:63
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marks the last message."
msgstr "<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Son mesajı işaretler."

#: src/app/templates/modals/hotkey.tpl.html:64
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Unmarks the message."
msgstr "<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Mesajın işaretini kaldırır."

#: src/app/templates/modals/hotkey.tpl.html:61
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Marks the <strong>previous</strong> message/conversation."
msgstr "<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> <strong>Önceki</strong> mesajı/ileti dizisini işaretler."

#: src/app/templates/modals/hotkey.tpl.html:38
msgctxt "Info"
msgid "<kbd>a</kbd> Moves the message/conversation to <strong>archive</strong>."
msgstr "<kbd>a</kbd> Mesajı/ileti dizisini <strong>arşive</strong> taşır."

#: src/app/templates/modals/hotkey.tpl.html:19
msgctxt "Info"
msgid "<kbd>c</kbd> Opens a new <strong>composer</strong>."
msgstr "<kbd>c</kbd> Yeni bir <strong>mesaj penceresi</strong> açar."

#: src/app/templates/modals/hotkey.tpl.html:21
msgctxt "Info"
msgid "<kbd>command</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>command</kbd> + <kbd>enter</kbd> <strong>Mesajı</strong> gönderir."

#: src/app/templates/modals/hotkey.tpl.html:23
msgctxt "Info"
msgid "<kbd>command</kbd> + <kbd>s</kbd> <strong>Saves</strong> the message."
msgstr "<kbd>command</kbd> + <kbd>s</kbd> Mesajı <strong>kaydeder</strong>."

#: src/app/templates/modals/hotkey.tpl.html:20
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Mesajı</strong> gönderir."

#: src/app/templates/modals/hotkey.tpl.html:22
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>s</kbd> <strong>Saves</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>s</kbd> Mesajı <strong>kaydeder</strong>."

#: src/app/templates/modals/hotkey.tpl.html:14
msgctxt "Info"
msgid "<kbd>enter</kbd> Confirms <strong>the active modal</strong>."
msgstr "<kbd>enter</kbd> <strong>Etkin pencereyi</strong> onaylar."

#: src/app/templates/modals/hotkey.tpl.html:67
msgctxt "Info"
msgid "<kbd>enter</kbd> Opens the <strong>marked</strong> message/conversation."
msgstr "<kbd>enter</kbd> <strong>İşaretli</strong> mesajı/ileti dizisini açar."

#: src/app/templates/modals/hotkey.tpl.html:68
msgctxt "Info"
msgid "<kbd>escape</kbd> Back to the list."
msgstr "<kbd>escape</kbd> Listeye geri döner."

#: src/app/templates/modals/hotkey.tpl.html:15
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes <strong>the active modal</strong>."
msgstr "<kbd>escape</kbd> <strong>Etkin pencereyi</strong> kapatır."

#: src/app/templates/modals/hotkey.tpl.html:24
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes the active <strong>composer</strong>."
msgstr "<kbd>escape</kbd> Etkin <strong>mesaj penceresini</strong> kapatır."

#: src/app/templates/modals/hotkey.tpl.html:54
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>.</kbd> Goes to <strong>starred</strong>."
msgstr "<kbd>g</kbd> ve <kbd>.</kbd> <strong>Yıldızlılara</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:55
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>a</kbd> Goes to <strong>archive</strong>."
msgstr "<kbd>g</kbd> ve<kbd>a</kbd> <strong>Arşive</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:52
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>d</kbd> Goes to <strong>drafts</strong>."
msgstr "<kbd>g</kbd> ve <kbd>d</kbd> <strong>Taslaklara</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:51
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>i</kbd> Goes to <strong>inbox</strong>."
msgstr "<kbd>g</kbd> ve <kbd>i</kbd> <strong>Gelenlere</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:53
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>s</kbd> Goes to <strong>sent</strong>."
msgstr "<kbd>g</kbd> ve <kbd>s</kbd> <strong>Gönderilenlere</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:57
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>t</kbd> Goes to <strong>trash</strong>."
msgstr "<kbd>g</kbd> ve <kbd>t</kbd> <strong>Çöpe</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:56
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>x</kbd> Goes to <strong>spam</strong>."
msgstr "<kbd>g</kbd> ve <kbd>x</kbd> <strong>Spama</strong> gider."

#: src/app/templates/modals/hotkey.tpl.html:36
msgctxt "Info"
msgid "<kbd>i</kbd> Moves the message/conversation to <strong>inbox</strong>."
msgstr "<kbd>i</kbd> Mesajı/İleti dizisini <strong>gelen kutusuna</strong> taşır."

#: src/app/templates/modals/hotkey.tpl.html:66
msgctxt "Info"
msgid "<kbd>j</kbd> Displays the <strong>older</strong> conversation."
msgstr "<kbd>j</kbd> <strong>Eski</strong> konuşmaları görüntüler."

#: src/app/templates/modals/hotkey.tpl.html:65
msgctxt "Info"
msgid "<kbd>k</kbd> Displays the <strong>newer</strong> conversation."
msgstr "<kbd>k</kbd> <strong>Yeni</strong> konuşmaları görüntüler."

#: src/app/templates/modals/hotkey.tpl.html:33
msgctxt "Info"
msgid "<kbd>r</kbd> Marks the message/conversation as <strong>read</strong>."
msgstr "<kbd>r</kbd> Mesajı/İleti dizisini <strong>okundu</strong> olarak işaretler."

#: src/app/templates/modals/hotkey.tpl.html:39
msgctxt "Info"
msgid "<kbd>s</kbd> Moves the message/conversation to <strong>spam</strong>."
msgstr "<kbd>s</kbd> Mesajı/İleti dizisini <strong>spama</strong> taşır."

#: src/app/templates/modals/hotkey.tpl.html:44
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>a</kbd> Reply to all recipients for a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>a</kbd> Bir mesaj/ileti dizisindeki tüm alıcıları yanıtlar."

#: src/app/templates/modals/hotkey.tpl.html:45
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>f</kbd> Forward a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>f</kbd> Bir mesajı/ileti dizisini yönlendirir."

#: src/app/templates/modals/hotkey.tpl.html:43
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>r</kbd> Reply to a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>r</kbd> Bir mesajı/ileti dizisini yanıtlar."

#: src/app/templates/modals/hotkey.tpl.html:37
msgctxt "Info"
msgid "<kbd>t</kbd> Moves the message/conversation to <strong>trash</strong>."
msgstr "<kbd>t</kbd> Mesajı/İleti dizisini <strong>çöpe</strong> taşır."

#: src/app/templates/modals/hotkey.tpl.html:34
msgctxt "Info"
msgid "<kbd>u</kbd> Marks the message/conversation as <strong>unread</strong>."
msgstr "<kbd>u</kbd> Mesajı/İleti dizisini <strong>okunmadı</strong> olarak işaretler."

#: src/app/templates/modals/hotkey.tpl.html:32
msgctxt "Info"
msgid "<kbd>x</kbd> Selects the message/conversation marked."
msgstr "<kbd>x</kbd> İşaretli mesajı/ileti dizisini seçer."

